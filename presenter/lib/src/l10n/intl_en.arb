{
  "deepLinkHttpsScheme": "https",
  "deepLinkAuthority": "instak4u.app",
  "deepLinkMerchant": "event/details/{eventDetails}",
  "@deepLinkMerchant": {
    "placeholders": {
      "eventDetails": {
        "type": "String"
      }
    }
  },
  "eventSharing": "*{title}*\n\n{description}\n\n\nLocation: {latitude}, {longitude}\nSee more: {url}",
  "@eventSharing": {
    "placeholders": {
      "title": {
        "type": "String"
      },
      "description": {
        "type": "String"
      },
      "latitude": {
        "type": "double"
      },
      "longitude": {
        "type": "double"
      },
      "url": {
        "type": "String"
      }
    }
  }
}